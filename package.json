    "next": "^15.5.4",
    "next-themes": "^0.4.6",
    "node-cron": "^4.2.1",
    "openai": "^6.3.0",
    "prisma": "^6.17.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "sonner": "^2.0.7",
