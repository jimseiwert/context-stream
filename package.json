{
  "name": "contextstream",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "deploy": "prisma migrate deploy && next start",
    "lint": "next lint",
    "worker": "NODE_OPTIONS='--max-old-space-size=2048' tsx src/lib/jobs/worker.ts",
    "jobs:clear": "tsx local_scripts/clear-jobs.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma generate && prisma migrate deploy",
    "db:migrate:create": "prisma migrate dev --create-only",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:seed:global-docs": "tsx scripts/seed-global-docs.ts",
    "db:fix:stuck-sources": "tsx local_scripts/fix-stuck-sources.ts",
    "db:fix:source-usage": "tsx local_scripts/fix-source-usage-counts.ts",
    "user:upgrade": "tsx scripts/upgrade-user-plan.ts",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "prepare": "node -e \"try { require('husky').install() } catch (e) { if (e.code !== 'MODULE_NOT_FOUND') throw e }\"",
    "setup:hooks": "bash scripts/setup-hooks.sh"
  },
  "dependencies": {
